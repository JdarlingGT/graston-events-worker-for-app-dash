{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "WebWorker"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noEmit": true,
    "types": ["@cloudflare/workers-types"],
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "wrangler.toml"],
  "exclude": ["node_modules", "dist"]
}